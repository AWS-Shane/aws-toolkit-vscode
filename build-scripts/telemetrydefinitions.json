{
    "metadata": [
        {
            "name": "lambdaruntime",
            "allowedValues": [
                "dotnetcore2.1",
                "nodejs12.x",
                "nodejs10.x",
                "nodejs8.10",
                "ruby2.5",
                "java8",
                "java11",
                "go1.x",
                "python3.8",
                "python3.7",
                "python3.6",
                "python2.7"
            ],
            "required": true,
            "description": "What lambda runtime was used in the operation"
        },
        {
            "name": "duration",
            "type": "int",
            "required": true,
            "description": "The duration of the operation in miliseconds"
        },
        {
            "name": "result",
            "allowedValues": ["succeeded", "failed", "cancelled"],
            "required": true,
            "description": "The result of the operation"
        }
    ],
    "metrics": [
        {
            "name": "lambda_delete",
            "description": "called when deleting lambdas remotely",
            "unit": "none",
            "metadata": ["$duration", "$result"]
        },
        {
            "name": "lambda_create",
            "description": "called when creating lambdas remotely",
            "unit": "none",
            "metadata": ["$lambdaruntime"]
        }
    ]
}
